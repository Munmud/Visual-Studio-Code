{
  "explorer.confirmDelete": false,
  "hts.general.maxTests": 200,
  "bracketPairColorizer.depreciation-notice": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.iconTheme": "material-icon-theme",
  // "workbench.sideBar.location": "right",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.wordWrap": "on",
  "editor.accessibilitySupport": "off",
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "code-runner.runInTerminal": true,
  // "C_Cpp.default.compilerPath": "C:\\MinGW\\bin\\gcc.exe",
  // "C_Cpp.updateChannel": "Insiders",
  "C_Cpp.default.compilerArgs": ["-std=c++17"],
  "cph.general.timeOut": 6000,
  // "cph.general.menuChoices": "cpp",
  "cph.general.useShortCodeForcesName": true,
  "cph.general.saveLocation": "E:/Competitive-Programming-Munmud/Bin",
  "cph.general.retainWebviewContext": true,
  "cph.language.python.Command": "py",
  "cph.language.cpp.Args": "-std=c++17 -D LOCAL",
  "cph.general.firstTime": false,
  "C_Cpp.default.compilerPath": "C:\\MinGW\\bin\\gcc.exe",
  "cph.general.defaultLanguageTemplateFileLocation": "E:/Competitive-Programming-Munmud/template.cpp",
  "cph.general.menuChoices": "",
  "cph.language.c.Args": "-D LOCAL",
  "cph.general.defaultLanguage": "cpp",
  "files.associations": {
    "*.cpp": "cpp"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
